{
  "expo": {
    "name": "IoT Sensors",
    "slug": "iot-sensors",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "scheme": "iot",
    "icon": "./assets/icons/image.png",
    "splash": {
      "image": "./assets/icons/image.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a202c"
    },
    "notification": {
      "icon": "./assets/icons/image.png",
      "color": "#f6ad55"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "iot.project.alt",
      "googleServicesFile": "./GoogleService-Info.plist",
      "icon": "./assets/icons/image.png"
    },
    "android": {
      "package": "iot.project.alt",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/icons/image.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/image.png",
        "backgroundColor": "#1a202c"
      },
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/icons/image.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/image.png",
          "color": "#f6ad55"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cf859017-5fb5-4e3e-a791-604051278c79"
      }
    }
  }
}
